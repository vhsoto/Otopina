<h1>Lista de procedimientos</h1>

<table>
  <thead>
    <tr>
      <th>Precio</th>
      <th>Subtotal</th>
      <th>Fecha</th>
      <th>Procedimiento</th>
      <th>Servicio</th>
      <th>Evento adverso</th>
      <th>Cargo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ae_procedures.each do |ae_procedure| %>
      <tr>
        <td><%= ae_procedure.procedure_price %></td>
        <td><%= ae_procedure.procedure_subtotal %></td>
        <td><%= ae_procedure.procedure_time %></td>
        <td><%= ae_procedure.procedure %></td>
        <td><%= ae_procedure.service %></td>
        <td><%= ae_procedure.adverse_event %></td>
        <td><%= ae_procedure.charge %></td>
        <td><%# link_to 'Ver', ae_procedure %></td>
        <td><%# link_to 'Editar', edit_ae_procedure_path(ae_procedure) %></td>
        <td><%= link_to 'Eliminar', adverse_event_ae_procedure_path(@adverse_event), method: :delete, data: { confirm: '¿Está seguro que quiere eliminar?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Crear procedimiento', new_adverse_event_ae_procedure_path(@adverse_event) %>
