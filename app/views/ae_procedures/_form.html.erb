<%= form_for([@adverse_event,@ae_procedure]) do |f| %>
  <% if @ae_procedure.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@ae_procedure.errors.count, "error") %> prohibited this ae_procedure from being saved:</h2>

      <ul>
      <% @ae_procedure.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :procedure_price, "Precio" %><br>
    <%= f.text_field :procedure_price %>
