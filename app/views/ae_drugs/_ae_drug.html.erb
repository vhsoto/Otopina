<tr>
  <td><%= fecha ae_drug.drug_time %> <br> <%= hora ae_drug.drug_time %></td>
  <td><%= ae_drug.drug.name %></td>
  <td><%= ae_drug.qty %></td>
  <td><%= number_to_currency(ae_drug.drug_price, delimiter: ".") %></td>
  <td><%= ae_drug.service.name %></td>
  <td><%= ae_drug.charge.name %></td>  
  <td><%= number_to_currency(ae_drug.drug_subtotal, delimiter: ".") %></td>
  <td ><%= link_to edit_adverse_event_ae_drug_path(ae_drug.adverse_event,ae_drug), class: "btn btn-info btn-sm pull-right" do %>
    <i class="fa fa-pencil"></i>
    <% end %>
  </td>  
</tr> 

