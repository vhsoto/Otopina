
        <tr>
          <td><%= fecha adverse_event.time %> <br> <%= hora adverse_event.time %></td> 
          <td><%= adverse_event.patient.document_number %></td>   
          <td><%= adverse_event.patient.name %> <%= adverse_event.patient.lastname %></td>                  
          <td><%= adverse_event.pregnant %></td>
          <td><%= adverse_event.event.Detail %></td>          
          <td><%= adverse_event.branch_office.name %></td>
          <td><%= link_to adverse_event, class: "btn btn-primary btn-sm" do %>
            <i class="fa fa-search"></i>
            <% end %>
          </td>
          <td><%= link_to edit_adverse_event_path(adverse_event), class: "btn btn-info btn-sm" do %>
            <i class="fa fa-pencil"></i>
            <% end %>
          </td>
          <td><%= link_to adverse_event, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger btn-sm" do %>
            <i class="fa fa-trash"></i>
            <% end %></td>
        </tr>